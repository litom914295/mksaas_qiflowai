    "replace:todos": "tsx scripts/replace-todo-placeholders.ts",
    "translate:todos": "tsx scripts/translate-todos-from-zh.ts",
    "translate:remaining": "tsx scripts/translate-remaining-todos.ts",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:credits": "vitest run tests/unit/credits",
    "test:security": "vitest run tests/security",
    "test:api": "vitest run tests/api",
    "test:i18n": "echo 'i18n-routes tests require Jest setup. See docs/test-scripts-setup.md'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:i18n": "playwright test e2e/i18n-navigation.spec.ts",
    "test:e2e:report": "playwright show-report",
    "playwright:install": "playwright install",
    "analyze": "node scripts/analyze-bundle.js",
    "optimize": "node scripts/dev-optimize.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "check-all": "tsc --noEmit && npm run lint",
    "content": "fumadocs-mdx",
    "email": "email dev --dir src/mail/templates --port 3333",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "knip": "knip",
    "seed:admin": "tsx scripts/seed-admin.ts",
    "backup:db": "tsx scripts/backup-database.ts",
    "backup:list": "tsx scripts/backup-database.ts list",
    "brand:verify": "powershell -ExecutionPolicy Bypass -File scripts/gates/verify-brand.ps1"
  },
